{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"simple": {
			"format": "%(asctime)s - %(name)s - %(level_name)s - %(message)s"
		}
	},
	
	"handlers": {
		"file_handler" : {
			"class": "logging.FileHandler",
			"level": "DEBUG",
			"formatter": "simple",
			"filename": "stealTheBasis.log",
			"encoding": "utf8"
		}
	},
	
	"root": {
		"level": "DEBUG",
		"handlers": ["file_handler"]
	}
}
